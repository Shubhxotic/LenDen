  <div class="cat-filters col-lg-2 col-xs-12">
		{{!-- <p><strong><center>Subcategories and Filters</center></strong></p> --}}
	 <div id="accordion">
    <div class="card">
      <a class="card-header accDiv" class=" text-black" data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
          Subcategories
      </a>
      <div id="collapseOne" class="collapse">
        <div class="list-group w-100 m-0">
          {{#each Subcategories}}
          <div class="list-group-item w-100">
            <a href="/subcategory/subcategory/{{this.Subcat_id}}" class="text-success">{{this.SubcategoryName}}</a><br>
            </div>
          {{/each}}
        </div>
      </div>
    </div>
    <div class="card">
